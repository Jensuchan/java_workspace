package day09;

public class ClassEx01 {

	public static void main(String[] args) {
		Point p = new Point(); //객체생성
		p.print();

	}

}

//(0,0)
class Point{  //일반적으로 클래스는 default로
	//멤버변수 자리
	int x;
	int y;
	
	//print 메서드
	//같은 클래스의 멤버변수는 모든 메서드에서 공유
	public void print() {
		System.out.println("("+x+","+y+")");
	}
	
}



