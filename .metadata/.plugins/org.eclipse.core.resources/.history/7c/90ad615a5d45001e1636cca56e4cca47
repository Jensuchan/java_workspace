package day09;

public class MethodEx02 {

	public static void main(String[] args) {
		/* 두 정수의 사칙연산(+ - * / %)
		 */
		MethodEx02 me = new MethodEx02();
		
		int a = 7, b= 13;
		
		System.out.println(me.add(a, b));
		System.out.println(me.sub(a, b));
		System.out.println(me.mul(a, b));
		System.out.println(me.div(a, b));
		System.out.println(me.rem(a, b));

	}
	//사칙 연산의 메서드 생성 static 없이
	public int add(int num1, int num2) {
		return num1+num2;
	}
	public int sub(int num1, int num2) {
		return num1-num2;
	}
	public int mul(int num1, int num2) {
		return num1*num2;
	}
	public double div(int num1, int num2) {
		return (double)num1/num2;
	}
	public int rem(int num1, int num2) {
		return num1%num2;
	}

}
